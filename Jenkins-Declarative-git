pipeline{
    agent any
     stages{
        stage("git"){
            steps{
            git branch: 'main', credentialsId: 'github', url: 'https://github.com/shaikafzal9866/jhc01.git'
        
         }
      }
      stage("maven bulid"){
            steps{
            sh "mvn clean package"
        
         }
      }    
   }
}
